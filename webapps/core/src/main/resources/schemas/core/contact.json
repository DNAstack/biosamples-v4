{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "https://www.ebi.ac.uk/biosamples/schemas/core/contact.json",
    "title": "Contact",
    "description": "Object representing a contact reference for the sample",
    "additionalProperties": false,
    "type": "object",
    "properties": {
        "FirstName": {
            "type": "string"
        },
        "LastName": {
            "type":"string"
        },
        "MidInitials": {
            "type":"string"
        },
        "Name": {
            "type":"string"
        },
        "Role": {
            "type":"string"
        },
        "E-mail": {
            "type":"string"
        },
        "Affiliation": {
            "type":"string"
        },
        "URL": {
            "type":"string"
        }
    }
}
